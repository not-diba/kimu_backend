type Recipe {
    amount: Int!
    categoryName: String!
    description: String!
    duration: String!
    id: ID!
    imageUrl: String!
    ingredients: [Ingredient!]
    instructions: [String]
    nutrition: [Nutrition!]
    people: Int!
    recipeName: String!
}

type Ingredient {
    name: String!
    quantity: String!
}

type Nutrition {
    nutritionItem: String!
    quantity: String!
}



type Query {
    recipes: [Recipe!]!
}